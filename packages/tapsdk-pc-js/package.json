{
  "name": "tapsdk-pc",
  "version": "0.2.0",
  "description": "Node.js bindings for TapTap PC SDK",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "*.node",
    "taptap_api.dll"
  ],
  "napi": {
    "name": "tapsdk-pc",
    "triples": {
      "defaults": false,
      "additional": [
        "x86_64-pc-windows-msvc"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^3.0.0-alpha.65",
    "@types/node": "^25.2.0",
    "typescript": "^5.7.0",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">= 20"
  },
  "packageManager": "pnpm@10.28.2",
  "scripts": {
    "build:native": "napi build --platform --release",
    "build:ts": "vite build",
    "build": "pnpm run build:native && pnpm run build:ts",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "taptap",
    "sdk",
    "game",
    "pc",
    "windows",
    "napi-rs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dsh0416/tapsdk-pc.js"
  }
}
